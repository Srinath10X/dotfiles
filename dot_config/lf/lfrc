set previewer ctpv
set cleaner ctpvclear
&ctpv -s $id
&ctpvquit $id
set icons true
set drawbox true
set cleaner ctpvclear
set hidden true
&ctpv -s $id
&ctpvquit $id

map r $nvim "$f"

cmd mkdir ${{
	printf "Directory name : "
	read dirName
	mkdir $dirName
}}

cmd touch ${{
	printf "File name : "
	read fileName
	touch $fileName
}}

cmd kitty ${{
	kitten icat "$f"
}}

cmd del ${{
	rm "$f"
}}

map gh cd ~/code/github/
map gc cd ~/.config/
map gb cd ~/
map i kitty
